<h1 align="center">Listing all categories</h1>
<div align="center">
  <%= will_paginate %>
  <ul class="listing">
    <% @categories.each do |category| %>
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li class="article-title">
            <%= link_to "#{category.name}", category_path(category) %>
          </li>
        </div>
      </div>
    <% end %>
    <%= will_paginate %>
  </ul>
</div>
